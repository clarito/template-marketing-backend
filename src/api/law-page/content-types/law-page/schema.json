{
  "kind": "singleType",
  "collectionName": "law_pages",
  "info": {
    "singularName": "law-page",
    "pluralName": "law-pages",
    "displayName": "LawPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "WebsiteTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "."
    },
    "UpdateTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "."
    },
    "Panels": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": ["law.law", "law.nested-law"],
      "required": true
    },
    "UrlTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true,
      "default": "."
    },
    "Title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true,
      "default": "."
    },
    "Desc": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "simple"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor",
      "default": "."
    },
    "ContentSeparator": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "unique": true,
      "options": {
        "value": "CONTENT"
      },
      "customField": "plugin::ui-separator.ui-separator"
    }
  }
}
